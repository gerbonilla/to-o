<div class="col s12 m6">
  <div class="card">
    <div class="card-image">
      <% photos = ["in1.png", "in2.png", "in3.png", "in4.png"] %>
      <!-- <img src="images/<%= photos.sample %>"> -->
      <%= image_tag(photos.sample); %>
    </div>
    <div class="card-content">
      <span class="card-title font-primary-color"><strong>Card Title</strong></span>
      <p class="truncate">$<%= rand(40...70) * 10 %>,000 en daños a terceros</p>
      <p class="truncate">$<%= rand(20...50) * 10 %> deducible pérdida parcial</p>
      <p class="truncate"><%= rand(8...18) %>% del valor asegurado deducible pérdida total</p>
    </div>
  </div>
</div>
